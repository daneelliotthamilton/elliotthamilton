<% if @partners.present? %>
<h2> Partners</h2>
<div class="row">
  <% @partners.each do |profile| %>
    <div class="col-sm-3 col-12">
      <%= link_to profile_path(profile) do %>
          <div class="profile-listing">
              <%= image_tag profile.avatar, class: "img-fluid mx-auto d-block", width: "250" %>
            <p class="text-center"> 
              <%= profile.title.upcase %>
            </p>
          </div>
      <% end %>
    </div>
  <% end %>
</div>
<% end %>
<% if @associates.present? %>
  <h2> Associates </h2>
  <div class="row">
    <% @associates.each do |profile| %>
      <div class="col-sm-3 col-12">
      <%= link_to profile_path(profile) do %>
          <div class="profile">
              <%= image_tag profile.avatar, class: "img-fluid mx-auto d-block", width: "200" %>
            <p class="text-center"> 
              <%= profile.title.upcase %>
            </p>
          </div>
      <% end %>
    </div>
    <% end %>
  </div>
<% end %>

